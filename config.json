{
  "$schema": "./source/schema.json",
  "family_name": "Maple Mono",
  "use_hinted": true,
  "pool_size": 4,
  "ligature": true,
  "feature_freeze": {
    "cv01": "disable",
    "cv02": "disable",
    "cv03": "disable",
    "cv04": "disable",
    "cv31": "disable",
    "cv32": "disable",
    "cv33": "disable",
    "cv34": "disable",
    "cv35": "disable",
    "cv36": "disable",
    "cv98": "disable",
    "cv99": "disable",
    "ss01": "disable",
    "ss02": "disable",
    "ss03": "enable",
    "ss04": "disable",
    "ss05": "enable",
    "ss06": "disable",
    "ss07": "enable",
    "ss08": "enable",
    "zero": "enable"
  },
  "nerd_font": {
    "enable": true,
    "version": "3.3.0",
    "mono": false,
    "use_font_patcher": false,
    "glyphs": [
      "--complete"
    ],
    "extra_args": []
  },
  "cn": {
    "enable": false,
    "with_nerd_font": true,
    "fix_meta_table": true,
    "clean_cache": false,
    "narrow": false,
    "use_hinted": false,
    "use_static_base_font": true
  }
}
